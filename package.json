{
  "name": "xmlbuilder2",
  "version": "1.1.0",
  "keywords": [
    "xml",
    "xmlbuilder"
  ],
  "homepage": "http://github.com/oozcitak/xmlbuilder2",
  "description": "An XML builder for node.js",
  "author": "Ozgur Ozcitak <oozcitak@gmail.com>",
  "contributors": [],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/oozcitak/xmlbuilder2.git"
  },
  "bugs": {
    "url": "http://github.com/oozcitak/xmlbuilder2/issues"
  },
  "main": "./lib/index",
  "engines": {
    "node": ">=8.0"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@oozcitak/util": "8.0.0",
    "@oozcitak/dom": "1.15.2",
    "@oozcitak/infra": "1.0.5"
  },
  "devDependencies": {
    "@types/benchmark": "*",
    "@types/chalk": "*",
    "@types/dedent": "*",
    "@types/jest": "*",
    "@types/node": "*",
    "benchmark": "*",
    "chalk": "*",
    "dedent": "*",
    "glob": "*",
    "jest": "*",
    "ts-jest": "*",
    "ts-node": "*",
    "typescript": "*",
    "xmlbuilder": "*",
    "xpath": "*"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "/test/.*\\.test\\.tsx?$",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ]
  },
  "scripts": {
    "compile": "rm -rf ./lib && tsc --version && tsc",
    "test": "npm run compile && jest --coverage",
    "perf": "npm run compile && ts-node ./perf/perf.ts",
    "prof-serialize": "npm run compile && rm -f isolate-*-v8.log && node --prof ./perf/prof-serialize.js && find . -name isolate-*-v8.log -exec mv {} isolate-v8.log ; && node --prof-process isolate-v8.log > isolate-serialize.log && rm isolate-v8.log",
    "publish-public": "npm run test && npm publish"
  }
}
